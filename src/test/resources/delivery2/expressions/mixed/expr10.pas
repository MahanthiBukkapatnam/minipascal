program Demo;
var
  x, y, z : integer;
begin
  z := (4 + 5 - 6);                             { 9 - 6                 = 3 }
  writeln(z);

  z := (3 + (4 + 5 - 6) - 7);                   { ( 3 + 3 - 7 = 6 - 7   = -1 }
  writeln(z);

  z := (2 + (3 + (4 + 5 - 6) - 7) * 8);         {  2 + (-1)* 8 = 2 - 8  = -6 }
  writeln(z);

  z := 1 * (2 + (3 + (4 + 5 - 6) - 7) * 8);     { ( 1 * -6              = -6 }
  writeln(z);

  z := ( ( (0-1)*900) * (2 + (3 + (4 + 5 - 6) - 7) * 8) / 9);   {  (-900 * -6) / 9 = 600 }
  writeln(z);
end.